{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "upload:default",
    "clipboard-manager:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "log:default",
    "fs:allow-app-meta",
    "fs:allow-app-meta-recursive",
    "fs:allow-write",
    "fs:allow-read"
  ]
}